<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ title }}</title>
    {{#if dataPost }}
    <meta name="description" content="{{ dataPost.resumen }}">
    <meta name="keywords" content="BlogSide,{{ toString dataPost.tags }}">
    <meta property="og:description" content="{{ dataPost.resumen }}" />
    <meta property="og:url" content="https://blogside.herokuapp.com/blog/{{ dataPost.url }}" />
    <meta property="og:image" content="{{ dataPost.baner }}" />
    <meta property="og:title" content="{{ title }}" />
    <meta name="twitter:card" content="summary">
    <meta name="twitter:url" content="https://blogside.herokuapp.com/blog/{{ dataPost.url }}">
    <meta name="twitter:title" content="{{ title }}">
    <meta name="twitter:description" content="{{ dataPost.resumen }}">
    <meta name="twitter:image" content="{{ dataPost.baner }}">
    <meta property="og:type" content="article" />
    {{/if}}
    {{#if dataSEO }}
    <meta name="description" content="{{ dataSEO.resumen }}">
    <meta name="keywords" content="BlogSide,{{ toString dataSEO.tags }}">
    <meta property="og:description" content="{{ dataSEO.resumen }}" />
    <meta property="og:url" content="https://blogside.herokuapp.com/{{ dataSEO.pagina }}" />
    <meta property="og:image" content="{{ dataSEO.img }}" />
    <meta property="og:title" content="{{ title }}" />
    <meta name="twitter:card" content="summary">
    <meta name="twitter:url" content="https://blogside.herokuapp.com/{{ dataSEO.pagina }}">
    <meta name="twitter:title" content="{{ title }}">
    <meta name="twitter:description" content="{{ dataSEO.resumen }}">
    <meta name="twitter:image" content="{{ dataSEO.img }}">
    <meta property="og:type" content="website" />
    {{/if}}
    <meta property="og:locale" content="es_MX" />
    <meta name="author" content="David Tovar">
    <link rel="apple-touch-icon" href="https://res.cloudinary.com/dgnwxv1ln/image/upload/v1648873616/apple-icon_vxkqw7.png">

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

    <!-- Bootstrap Icons -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">

    <!-- Axios.js -->
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    
    <!-- JQuery -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>

    <!-- JQuery UI -->
    <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/themes/smoothness/jquery-ui.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>

    <!-- TinyMCE -->
    <script src="https://cdn.tiny.cloud/1/jm19upx3bpuf0y5hxthhds3dk1ykoospxubwaf3hkc2945ad/tinymce/5/tinymce.min.js" referrerpolicy="origin"></script>

    <!-- Particle.js -->
    <script src="/js/particles.min.js"></script>

    {{!-- Local JS --}}
    <script src="/js/main.js"></script>

    {{!-- Local CSS --}}
    <link rel="stylesheet" href="/css/styles.css">
</head>
<body>